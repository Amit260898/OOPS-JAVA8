package Inheritance;

public class Main {
	
	public static void main(String[] args) {
		
		Box box1=new Box(4.4,6.7,1.2);
		Box box2=new Box(box1);
		
		System.out.println(box1.l+" "+box1.w+" "+box1.h);
		
		BoxWeight box3=new BoxWeight(3);
		System.out.println(box3.weight+ " "+ box3.h);
		
	}

}
