package Interface;

public class NiceCar {
	
	private Engine engine;
	
	public NiceCar() {
		
		engine=new PowerEngine();
	}

	public NiceCar(Engine engine) {
		super();
		this.engine = engine;
	}
	
	public void start() {
		engine.start();
	}
	
	public void stop() {
		engine.stop();
	}

}
