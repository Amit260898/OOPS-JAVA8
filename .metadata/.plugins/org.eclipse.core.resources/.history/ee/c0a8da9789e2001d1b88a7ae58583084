package main;

import java.util.HashMap;
import java.util.Iterator;
import java.util.Set;

public class MapHashMap {
	
	public static void main(String[] args) {
		
		HashMap hm =new HashMap<>();
		
		hm.put(101, "Amit");
		hm.put(102, "Sumit");
		hm.put(103, "Namit");
		hm.put(104, "Ramit");
		
		Set set=hm.entrySet();
		System.out.println(set);
		
		
	}

}
